{
  "name": "cloudify-stage",
  "version": "5.1.0-.dev1",
  "description": "Cloudify Console",
  "repository": "https://github.com/cloudify-cosmo/cloudify-stage",
  "license": "Apache-2.0",
  "author": "Cloudify",
  "files": [
    "backend",
    "dist",
    "conf/app.json",
    "conf/manager.json",
    "conf/README.md",
    "conf/userConfig.json"
  ],
  "scripts": {
    "beforebuild": "npm ci --prefer-offline && (cd backend && npm ci --prefer-offline)",
    "build": "webpack --config webpack.config-prod.js --bail",
    "devServer": "node devServer.js",
    "devtest": "mocha --require @babel/register --require esm --require ./test/setup.js -R spec",
    "docCheck": "bash scripts/checkDocs.sh",
    "docWidgets": "node scripts/updateReadmes.js",
    "e2e": "npm run e2e:old && npm run e2e:new",
    "e2e:new": "cypress run --reporter cypress-multi-reporters --reporter-options configFile=test/cypress/reporter-config.json",
    "e2e:new:dev": "cypress run --reporter spec",
    "e2e:new:open": "cypress open",
    "e2e:old": "nightwatch -c ./test/nightwatch/nightwatch.conf.js",
    "e2e:old:dev": "nightwatch -c ./test/nightwatch/nightwatch.conf.js --env dev",
    "lint": "eslint app backend widgets test scripts --ext .js",
    "prodtest": "mocha --require @babel/register --require esm --require ./test/setup.js -R spec --recursive \"./test/**/*Test.js\"",
    "size": "size-limit",
    "test": "npm run lint && npm run prodtest && npm run size",
    "upload": "bash scripts/uploadPackage.sh",
    "zip": "bash scripts/createPackage.sh"
  },
  "dependencies": {
    "better-console": "^0.2.4",
    "blueimp-md5": "^2.7.0",
