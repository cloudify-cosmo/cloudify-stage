    "e2ev": "nightwatch -c ./e2e/nightwatch.conf.js --verbose",
    "beforebuild": "scripts/preinstall.sh",
    "build": "webpack --config webpack.config-prod.js --bail",
    "zip": "scripts/createPackage.sh",
    "size": "size-limit"
