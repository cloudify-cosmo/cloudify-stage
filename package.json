{
  "name": "cloudify-stage",
  "version": "5.2.0-.dev1",
  "description": "Cloudify Console",
  "homepage": "https://github.com/cloudify-cosmo/cloudify-stage#readme",
  "repository": "https://github.com/cloudify-cosmo/cloudify-stage",
  "license": "Apache-2.0",
