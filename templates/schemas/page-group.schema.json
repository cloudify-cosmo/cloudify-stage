{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "description": "Page Group definition",
  "properties": {
    "name": {
      "type": "string",
      "description": "Page Group name"
    },
    "icon": {
      "type": "string",
      "description": "Page Group icon (one of Semantic UI React icons)"
    },
    "pages": {
      "type": "array",
      "minItems": 1,
      "description": "List of Pages",
      "items": {
        "type": "string",
        "description": "Page ID"
      }
    }
  },
  "required": [
    "name",
    "pages"
  ]
}
