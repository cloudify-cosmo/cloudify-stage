<% if (Array.isArray(variables) && variables.length) { _%>
        <%= name %>:
<% } _%>
<% variables.forEach(variable => { _%>
          <%= variable.variable %>: <% if (variable.source === "input" || variable.source === "secret") { _%>{ get_<%=
