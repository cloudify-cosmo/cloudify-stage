  "urlAuthentication": true,
  "terraformVersion": "1.2.2",
  "terraformTemplate": "http://host/path/template.zip",
  "resourceLocation": "/ft_folder/template",
  "variables": [
    {
      "name": "variableA",
      "source": "secret",
      "value": "secretA",
      "default": "defaultA"
    },
    {
      "name": "variableB",
      "source": "input",
      "value": "inputA",
      "default": "defaultB"
    },
    {
      "name": "variableD",
      "source": "static",
      "value": "",
      "default": "defaultD"
    }
  ],
  "environmentVariables": [
    {
      "name": "variableC",
      "source": "input",
      "value": "inputC",
      "default": "defaultC"
    }
  ],
  "outputs": [
    {
      "name": "outputA",
      "type": "output",
      "terraformOutput": "outA"
    },
    {
      "name": "outputB",
      "type": "capability",
      "terraformOutput": "outB"
    }
  ]
}
